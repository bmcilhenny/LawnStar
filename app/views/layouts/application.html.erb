<!DOCTYPE html>
<html>
  <head>
    <title>LawnStar</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
     <br>
     <% if logged_in? %>
      <%=link_to "Log a Game", new_game_path%> | <%=link_to "Add a Sport", new_sport_path%> | <%= link_to "Return to Player Page", player_path(@current_player)%> | <%= link_to "Return to League Page", league_path(@current_player.league)%> | <%=link_to "Sign Out", sessions_path %><br>
    <%end%>
     <br>
    <%= yield %>
  </body>
</html>
